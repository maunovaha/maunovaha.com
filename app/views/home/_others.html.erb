        </div>
        <div class="info-overlay">
          <i class="fa fa-external-link"></i>
          <span class="title">ylivieskankivimiehet.fi</span>
          <span class="description">Code & design.</span>
        </div>
      <% end %>
    </div>

    <div class="pure-u-1 pure-u-cc-1-2 pure-u-md-1-3">
      <%= link_to "http://rakennuspalveluhuikari.fi", target: "_blank", class: "img-link" do %>
        <div class="img-wrapper">
          <img src="/assets/others/rakennuspalvelu-huikari.png" class="pure-img" alt="Rakennuspalvelu Huikari">
        </div>
        <div class="info-overlay">
          <i class="fa fa-external-link"></i>
          <span class="title">rakennuspalveluhuikari.fi</span>
